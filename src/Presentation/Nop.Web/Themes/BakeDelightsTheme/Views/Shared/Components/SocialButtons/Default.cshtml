@model SocialModel
<ul class="nav nav-list social-wrapper">
    @if (!string.IsNullOrEmpty(Model.FacebookLink))
    {
        <li class="facebook"><a href="@Model.FacebookLink" target="_blank"><i class="fab fa-facebook"></i></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.TwitterLink))
    {
        <li class="twitter"><a href="@Model.TwitterLink" target="_blank"><i class="fab fa-twitter"></i></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.GooglePlusLink))
    {
        <li class="instagram"><a href="@Model.GooglePlusLink" target="_blank"><i class="fab fa-instagram"></i></a></li>
    }
    @if (!string.IsNullOrEmpty(Model.YoutubeLink))
    {
        <li class="telegram"><a href="@Model.YoutubeLink" target="_blank"><i class="fab  fa-telegram"></i></a></li>
    }
    @if (Model.NewsEnabled)
    {
        <li class="rss"><a href="@Url.RouteUrl("NewsRSS", new { languageId = Model.WorkingLanguageId })"><i class="fa  fa-rss"></i></a></li>
    }
</ul>